<extentreports>
	<configuration>
		<theme>dark</theme>
		<encoding>UTF-8</encoding>
		<protocol>https</protocol>
		<documentTitle>PCube_UI_Automation</documentTitle>
		<reportHeadline>PCube Automation Report</reportHeadline>
		<reportName>
			<![CDATA[<img src="https://i.ibb.co/6JbNSZK/Logo.jpg" alt="Logo" border="0">]]>
		</reportName>
		<dateFormat>yyyy-MM-dd</dateFormat>
		<timeFormat>HH:mm:ss</timeFormat>
		<scripts>
        <![CDATA[
            $(document).ready(function() {
            $("body").addClass("dark");
            });
        ]]>
		</scripts>
		<scripts>
        <![CDATA[
           $('.charts > div:nth-child(2)').hide();
        ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
            <![CDATA[
                .report-name { padding-left: 10px; } .report-name > img { float: left;height: 90%;margin-left: 30px;margin-top: 2px;width: auto; }
            ]]>
		</styles>
	</configuration>
</extentreports>